<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config
3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>

	<typeAliases>
		<typeAlias alias="board" type="com.green.teddy.dto.Board" />
		<typeAlias alias="car" type="com.green.teddy.dto.Car" />
		<typeAlias alias="help" type="com.green.teddy.dto.Help" />
		<typeAlias alias="member" type="com.green.teddy.dto.Member" />
		<typeAlias alias="news" type="com.green.teddy.dto.News" />
		<typeAlias alias="reservation" type="com.green.teddy.dto.Reservation" />
		<typeAlias alias="review" type="com.green.teddy.dto.Review" />
		<typeAlias alias="center" type="com.green.teddy.dto.Center" />
		<typeAlias alias="design_img" type="com.green.teddy.dto.Design_img"/>
		<typeAlias alias="compliment" type="com.green.teddy.dto.Compliment"/>
		<typeAlias alias="notice" type="com.green.teddy.dto.Notice"/>
	</typeAliases>

	<mappers>
		<mapper resource="member.xml" />
		<mapper resource="car.xml" />
		<mapper resource="help.xml" />
		<mapper resource="news.xml" />
		<mapper resource="reservation.xml" />
		<mapper resource="review.xml" />
		<mapper resource="center.xml" />
		<mapper resource="design_img.xml" />
		<mapper resource="board.xml" />
	</mappers>

</configuration>

